{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Space Girl Bot",
            "type": "debugpy",
            "request": "launch",
            "module": "src.spacegirl",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
